{
    "name": "chrome-tree-tab-manager",
    "version": "0.1.0",
    "description": "Chrome树状标签管理器 - 以树状结构展示和管理标签页",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "preview": "vite preview",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "type-check": "vue-tsc --noEmit",
        "lint": "eslint . --ext .ts,.tsx,.vue"
    },
    "dependencies": {
        "vue": "^3.4.0",
        "pinia": "^2.1.7"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^5.0.0",
        "@vue/tsconfig": "^0.5.0",
        "@vue/test-utils": "^2.4.0",
        "typescript": "^5.3.0",
        "vite": "^5.0.0",
        "vue-tsc": "^1.8.27",
        "vitest": "^1.1.0",
        "@vitest/ui": "^1.1.0",
        "jsdom": "^23.0.0",
        "@types/node": "^20.10.0",
        "@types/chrome": "^0.0.253",
        "@crxjs/vite-plugin": "^2.0.0-beta.21",
        "eslint": "^8.55.0",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "eslint-plugin-vue": "^9.19.0"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}